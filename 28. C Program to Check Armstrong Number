#include <stdio.h>
#include <math.h>
int main() {
    int num,originalNum,remainder,n = 0;
    float result=0.0;
    printf("Enter an integer:");
    scanf("%d",&num);
    originalNum=num;
  for(int i=num;i!=0;i/=10){
        n++;
    }
    for (int i=num;i!=0;i/=10){
        remainder=i%10;
        result+=pow(remainder,n);
    }
    if ((int)result==num){
        printf("Number is Armstrong number.%d\n",num);
    }
    else
        printf(" Number is not Armstrong number.%d\n",num);
    return 0;
}
