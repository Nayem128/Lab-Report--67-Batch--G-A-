#include <stdio.h>
int main(){
    int n,d;
    printf("Enter the size of the array:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter %d elements:",n);
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Enter number of positions to rotate:");
    scanf("%d",&d);
    int temp[d]; 
    for (int i=0;i<d;i++) {
        temp[i]=arr[i];
    }
    for(int i=d;i<n;i++) {
        arr[i-d]=arr[i];
    }
    for(int i=0;i<d;i++) {
        arr[n-d+i]=temp[i];
    }
    printf("Array after rotation:");
    for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }
    printf("\n");
    return 0;
}
