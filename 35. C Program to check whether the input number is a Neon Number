#include <stdio.h>
int main() {
    int num,square,sum=0,temp;
    printf("Enter a number:");
    scanf("%d",&num);
    square=num*num;
    temp=square;
    while(temp>0){
        sum+=temp%10;
        temp/=10;
    }
    if (sum == num) {
        printf("Number is a Neon Number.%d\n", num);
    } else {
        printf("Number is not a Neon Number.%d\n", num);
    }
    return 0;
}
