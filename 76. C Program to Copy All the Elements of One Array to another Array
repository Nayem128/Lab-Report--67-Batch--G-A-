#include <stdio.h>
int main(){
    int n;
    printf("Array size:");
    scanf("%d",&n);
    int a[n],b[n];
    printf("Enter elements of first array:");
    for(int i=0;i<n;i++) 
    scanf("%d",&a[i]);
    for(int i=0;i<n;i++) 
    b[i]=a[i]; 
    printf("Elements of second array:");
    for(int i=0;i<n;i++) 
    printf("%d ",b[i]);
    return 0;
}
