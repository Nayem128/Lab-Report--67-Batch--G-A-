#include <stdio.h>
int main(){
    int a[10][10],r,c;
    printf("Enter number of rows and columns:");
    scanf("%d %d",&r,&c);
    printf("Enter elements:\n");
    for(int i=0;i<r;i++){
        for(int j=0;j<c; j++) {
            scanf("%d",&a[i][j]);
        }
    }
    printf("\n2D Array:\n");
    for(int i=0;i<r;i++) {
        for(int j=0;j<c;j++) {
            printf("%d ",a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
