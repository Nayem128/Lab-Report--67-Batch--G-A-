#include <stdio.h>
#include <math.h>
int main(){
    int num,temp,remainder,n;
    float result;
    printf("Armstrong numbers between 1 and 1000 are:");
    for(num=1;num<=1000;num++){
        temp=num;
        result=0.0;
        n=0;
        for(int i=temp;i!=0;i/=10){
            n++;
        }
        for(int i=temp;i!=0;i/=10){
            remainder=i%10;
            result+=pow(remainder,n);
        }
        if ((int) result==num){
            printf("%d ",num);
        }
    }
   return 0;
}
