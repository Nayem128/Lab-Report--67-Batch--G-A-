#include<stdio.h>
int isPrime(int num){
    if(num<=1){
        return 0;
    }
    for(int i=2;i*i<=num;i++){
        if(num%i==0){
            return 0;
        }
    }
    return 1; 
}
int main(){
    int n;
    printf("Many Number check:");
    scanf("%d", &n);
    int num;
    for(int i=0;i<n;i++){
        printf("Enter number%d:",i+1);
        scanf("%d",&num);
        if(isPrime(num)) {
            printf("%d is a prime number.\n",num);
        } else {
            printf("%d is not a prime number.\n",num);
        }
    }
    return 0;
}
