#include <stdio.h>
int main() {
    int n1,n2;
    printf("Size of 1st array:");
    scanf("%d",&n1);
    int a[n1];
    printf("Enter elements of 1st array:");
    for(int i=0;i<n1;i++) 
    scanf("%d",&a[i]);
    printf("Size of 2nd array:");
    scanf("%d",&n2);
    int b[n2];
    printf("Enter elements of 2nd array:");
    for(int i=0;i<n2;i++)
    scanf("%d",&b[i]);
    int c[n1+n2];
    for(int i=0;i<n1;i++) 
    c[i]=a[i];
    for(int i=0;i<n2;i++)
    c[n1+i]=b[i];
    printf("Merged array:");
    for(int i=0;i<n1+n2;i++) 
    printf("%d ", c[i]);
    return 0;
}
