#include <stdio.h>
#include <math.h>
int main(){
    int low,high,num,temp,remainder,n;
    float result;
    printf("Enter two numbers (intervals):");
    scanf("%d%d",&low,&high);
    printf("Armstrong numbers between %d and %d are:\n", low, high);
    for(num=low;num<=high;num++){
        temp=num;
        result=0.0;
        n=0;
        for(int i=temp; i!=0;i/=10){
            n++;
        }
        for(int i=temp;i!=0;i/=10) {
            remainder=i%10;
            result+=pow(remainder,n);
        }
        if ((int)result==num){
            printf("%d ",num);
        }
    }
    return 0;
}
