#include <stdio.h>
int main(){
    int n,i,a=0,b=1,sum=0;
    printf("Enter number of terms:");
    scanf("%d",&n);
    for(i=0;i<n;i++) {
        if(i%2==0){
            sum+=a;
        }
        b=a+b;  
        a=b-a;  
    }
    printf("Sum of Fibonacci numbers at even indexes:%d\n",sum);
    return 0;
}
