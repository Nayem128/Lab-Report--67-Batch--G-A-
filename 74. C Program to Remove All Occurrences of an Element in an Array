#include <stdio.h>
int main(){
    int n,num;
    printf("Array size:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter elements:");
    for(int i=0;i<n;i++) 
    scanf("%d",&arr[i]);
    printf("Enter element to remove:");
    scanf("%d",&num);
    int j=0;
    for(int i=0;i<n;i++)
        if(arr[i]!=num)
            arr[j++]=arr[i];
    printf("Array after removing %d:",num);
    for(int i=0;i<j;i++) 
    printf("%d ",arr[i]);
    return 0;
}
