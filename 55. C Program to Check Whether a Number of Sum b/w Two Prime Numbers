#include <stdio.h>
int isPrime(int n){
    int i;
    if(n<=1)
        return 0;
    for(i=2;i*i<=n;i++){
        if(n%i==0)
            return 0;
    }
    return 1;
}
int main(){
    int n,i,flag=0;
    printf("Enter a positive integer:");
    scanf("%d",&n);
    for(i=2;i<=n/2;i++) {
        if(isPrime(i)&&isPrime(n-i)){
            printf("%d=%d+%d\n",n,i,n-i);
            flag = 1;
        }
    }
    if(flag == 0)
        printf("cannot be expressed as sum of two prime numbers.%d\n",n);
    return 0;
}
